{{<layout }}

{{$contents}}
    <div class="row">
        <div class="col-md-12">
        <h3>{{name}}</h3>
			<div class="table-responsive" id="routes">
				<table class="table">
					<col width="50%">
					<col width="50%">
					<tbody>
						{{#make_two_columns routes}}
						<td data-ref="{{id}}"><a href="view/{{id}}">{{title}}</a></td>
					{{/make_two_columns}}	
					</tbody>
				</table>
			</div> 
           </div>
        </div>
    </div>
{{/contents}}
{{/layout}}
